{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/tscont/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/tscont",
        "main": "packages/tscont/src/index.ts",
        "tsConfig": "packages/tscont/tsconfig.lib.json",
        "assets": ["packages/tscont/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/tscont/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/tscont"],
      "options": {
        "jestConfig": "packages/tscont/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
